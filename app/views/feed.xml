<Alloy>
	<Tab id="feedTab" title=" Live Blog ">
		<Window id="feedWindow" class="topBar" title="JMS 2015 Live Blog" onOpen="doOpen">
			<TitleControl platform="ios">
				<Label class="titolo">The John Main Seminar 2015</Label>
			</TitleControl>
			<LeftNavButton platform="ios">
				<ImageView class="leftNavImg"/>
			</LeftNavButton>
			<RightNavButton platform="ios">
				<ImageView class="refreshBlog" onClick="refreshLiveBlog"/>
			</RightNavButton>
			<WebView id="feedWebView"/>
		</Window>
	</Tab>
</Alloy>